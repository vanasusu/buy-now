<section>
  <div class="container mt-3">
    {{#if users}}
      {{!-- <h3 class="text-center">All Users</h3> --}}
       <table class="table">
          <thead>
            <tr>
              <th scope="col">Username</th>
              <th scope="col">Email</th>
            </tr>
          </thead>
          <tbody>
      {{#each users}}
       
            <tr>
                <td>{{this.username}}</td>
                <td>{{this.email}}</td>
            </tr>
        {{/each}}     
          </tbody>


        </table>
   
      {{!-- {{/each}} --}}
    {{else}}
      <h3 class="text-center">There are no users</h3>
    {{/if}}
  </div>
</section>